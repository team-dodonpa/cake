<h1>商品編集</h1>
<%= form_with model: @item,url:admin_items_path, local:true do |f| %>
  <h4>商品画像</h4>
  <%= f.attachment_field :image %>
  <h4>商品名</h4>
  <%= f.text_field :item_name %>
  <h4>商品説明</h4>
  <%= f.text_area :caption %>
  <h4>ジャンル</h4>
  <%#<%= form.collection_select() %>
  <h4>税抜き価格</h4>
   <%= f.text_field :item_ %>円
   <h4>販売ステータス</h4>
     <%= f.radio_button :is_active, "販売中" %>販売中 <%= f.radio_button :is_active, "販売停止中" %>販売停止中 
  <%= f.submit '変更を保存' %>
  
<% end %>