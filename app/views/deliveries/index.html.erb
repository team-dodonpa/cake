<%= form_with model:@delivery, local:true do |f| %>

  <h4>郵便番号</h4>
  <%= f.text_field :postal_code %>

  <h4>住所</h4>
  <%= f.text_field :address %>

  <h4>宛先</h4>
  <%= f.text_field :address_name %>

  <%= f.submit '投稿' %>

<% end %>
<% @deliveries.each do |delivery| %>
 <%= delivery.postal_code %>
 <%= delivery.address %>
 <%= delivery.address_name %>
 <% end %>
